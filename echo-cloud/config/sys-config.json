{
  "keyStorePath": "keystore.p12",
  "keyStorePassword": "123456",
  "keyPassword": "123456",
  "trustStorePath": "truststore.p12",
  "trustStorePassword": "123456",
  "interfaces": [
    "HTTP-SECURE-HTML",
    "HTTP-SECURE-JSON"
  ],
  "serviceRegistryHost": "service-registry.local:8443",
  "providers": [
    {
      "address": "echo-provider.local",
      "authenticationInfo": "",
      "port": 9001,
      "systemName": "echo_provider"
    },
    {
      "address": "echo-consumer.local",
      "authenticationInfo": "",
      "port": 9002,
      "systemName": "echo_consumer"
    }
  ],
  "services": [
    {
      "serviceDefinition": "kalix-example-provider-service",
      "providerIndex": 0,
      "serviceUri": "/example",
      "secure": "TOKEN",
      "interfaces": [
        "HTTP-SECURE-JSON"
      ]
    },
    {
      "$comment": "This service must exist (Arrowhead 4.3.0) in order to ensure that the echo_consumer system is actually registered. The reason for this is that there is no way to register a system without also specifying a service.",
      "serviceDefinition": "kalix-example-consumer-dummy",
      "providerIndex": 1,
      "serviceUri": "/dummy",
      "secure": "CERTIFICATE",
      "interfaces": [
        "HTTP-SECURE-JSON"
      ]
    }
  ],
  "rules": [
    {
      "consumer": "echo_consumer",
      "services": [
        "kalix-example-provider-service"
      ],
      "providers": [
        "echo_provider"
      ]
    }
  ]
}